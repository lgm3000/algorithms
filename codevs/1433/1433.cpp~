#include<iostream>
using namespace std;

int main(){
	int t,n;
	cin >> t;
	for(int mana = 0; mana<t; mana++){
		cin >> n;
		int r[200] = {0}, c[200] = {0},tmp;
		for(int i = 0;i<n;i++){
			for(int j = 0;j<n;j++){cin >> tmp;r[i]+=tmp;c[j]+=tmp;}
		}
		for(int i = 0;i<n;i++) cout << r[i] <<" ";
		cout << endl;
		for(int i = 0;i<n;i++) cout << c[i] <<endl;

	}


}
